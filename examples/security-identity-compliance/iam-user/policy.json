{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:PutObject"
            ],
            "Resource": [
                "arn:aws:s3:::some-bucket/some-folder/somesub_folder/",
                "arn:aws:s3:::some-bucket/some-folder/somesub_folder/*"
            ]
        }
    ]
}